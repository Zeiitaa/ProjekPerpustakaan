triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `insertdatabuku` AFTER INSERT ON `buku` FOR EACH ROW BEGIN\nDECLARE KET VARCHAR(500);\nSET KET = CONCAT (\'DITAMBAHKAN DATA BUKU BARU ID = \', new.id, \' JUDUL BUKU = \', new.judul, \' KATEGORI BUKU = \', new.id_kategori, \' PENGARANG = \', new.pengarang, \'DESKRIPSI BUKU = \', new.deskripsi, \' TAHUN TERBIT = \', new.tahun_terbit, \'JUMLAH BUKU = \', new.jumlah, \'PATH COVER = \', new.gambar, \'READTIME = \', new.readtime);\n\nINSERT INTO logaktifitas (USERNAME, TIPE, OPERASI, WAKTU) VALUES (\'admin\',\'INSERT\',KET, NOW());\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `updatedatabuku` AFTER UPDATE ON `buku` FOR EACH ROW BEGIN\nDECLARE KET VARCHAR(1000);\nSET KET = CONCAT (\'DIUPDATE DATA BUKU BARU ID = \', old.id, \' JUDUL BUKU = \', old.judul, \'MENJADI = \', new.judul, \n\'KATEGORI BUKU = \', old.id_kategori, \'MENJADI = \',new.id_kategori, \n\' PENGARANG = \', old.pengarang, \'MENJADI = \', new.pengarang, \n\'DESKRIPSI BUKU = \', old.deskripsi, \'MENJADI = \', new.deskripsi, \n\' TAHUN TERBIT = \', old.tahun_terbit, \'MENJADI = \', new.tahun_terbit, \n\'JUMLAH BUKU = \', old.jumlah, \'MENJADI = \', new.jumlah, \n\'PATH COVER = \', old.gambar, \'MENJADI = \', new.gambar, \n\'READTIME = \', old.readtime, \'MENJADI = \', new.readtime);\n\nINSERT INTO logaktifitas (USERNAME, TIPE, OPERASI, WAKTU) VALUES (\'admin\',\'UPDATE\',KET, NOW());\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `hapusdatabuku` AFTER DELETE ON `buku` FOR EACH ROW BEGIN\nDECLARE KET VARCHAR(500);\nSET KET = CONCAT (\'DIHAPUS DATA BUKU BARU ID = \', old.id, \' JUDUL BUKU = \', old.judul, \' KATEGORI BUKU = \', old.id_kategori, \' PENGARANG = \', old.pengarang, \'DESKRIPSI BUKU = \', old.deskripsi, \' TAHUN TERBIT = \', old.tahun_terbit, \'JUMLAH BUKU = \', old.jumlah, \'PATH COVER = \', old.gambar, \'READTIME = \', old.readtime);\n\nINSERT INTO logaktifitas (USERNAME, TIPE, OPERASI, WAKTU) VALUES (\'admin\',\'DELETE\',KET, NOW());\nEND'
